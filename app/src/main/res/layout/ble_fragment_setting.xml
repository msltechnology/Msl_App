<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/readData_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="fragment_setting" />

    <TextView
        android:id="@+id/tv_ble_framgment_setting_selected_ID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tv_ble_framgment_setting_selected_ID" />

    <TextView
        android:id="@+id/tv_ble_framgment_setting_selected_FL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tv_ble_framgment_setting_selected_FL" />



    <Button
        android:id="@+id/btn_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Status" />

    <Button
        android:id="@+id/btn_ID_Setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ID_setting" />
    <Button
        android:id="@+id/btn_FL_Setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/FL_setting" />

</LinearLayout>